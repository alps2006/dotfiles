defaults
logfile /var/log/msmtp.log

account gmail
host smtp.gmail.com
from s5unty@gmail.com
port 587
user s5unty@gmail.com
password <PASSWORD>
tls on
tls_certcheck off
auth on

account company
host www.foo.com
from email@foo.com
user email@foo.com
password <PASSWORD>
auth on

account default: gmail
