image/*; qiv %s;

application/x-diff-gzip; atool -l %s; copiousoutput;
application/x-gtar; atool -l %s; copiousoutput;
application/x-tar; atool -l %s; copiousoutput;
application/x-tar-gz; atool -l %s; copiousoutput;
application/x-rar-compressed; atool -l %s; copiousoutput;
application/x-zip-compressed; atool -l %s; copiousoutput;
application/x-gzip; atool -F tar+gzip -l %s; copiousoutput;
application/zip; atool -l %s; copiousoutput;

#application/vnd.ms-excel; xlhtml %s | elinks -dump -dump-width 200; copiousoutput; description=Microsoft Excel Document; nametemplate=%s.xls;
#application/ms-excel; xlhtml %s | elinks -dump -dump-width 200; copiousoutput; description=Microsoft Excel Document; nametemplate=%s.xls;
#application/msexcel; xlhtml %s | elinks -dump -dump-width 200; copiousoutput; description=Microsoft Excel Document; nametemplate=%s.xls;
