<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Debian Packages - Contents</os:ShortName>
<os:Description>Search Debian Package Contents</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/update.php/id0/debfilefind.src</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/update.php/id0/debfilefind.png</IconUpdateUrl>
<SearchForm>http://www.debian.org/distrib/packages#search_contents</SearchForm>
<os:Url type="text/html" method="GET" template="http://packages.debian.org/search?searchon=contents&amp;keywords={searchTerms}&amp;mode=filename&amp;suite=stable&amp;arch=any">
</os:Url>
</SearchPlugin>