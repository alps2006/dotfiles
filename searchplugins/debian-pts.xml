<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Debian PTS</os:ShortName>
<os:Description>Perform a search for Debian source packages</os:Description>
<os:InputEncoding>utf-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/update.php/id0/debpts.src</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/update.php/id0/debpts.png</IconUpdateUrl>
<SearchForm>http://packages.qa.debian.org/</SearchForm>
<os:Url type="text/html" method="GET" template="http://packages.qa.debian.org/?src={searchTerms}">
</os:Url>
</SearchPlugin>