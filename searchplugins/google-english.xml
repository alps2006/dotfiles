<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google.com (in English)</os:ShortName>
<os:Description>Google.com that doesn't default to local Google site when outside US, without restricting search to the English language</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<SearchForm>http://www.google.com/</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.google.com/search">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="hl" value="en"/>
</os:Url>
</SearchPlugin>